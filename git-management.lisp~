(defun get-git-path-string ()
  (string-trim 
      '(#\Space #\Newline #\Backspace #\Tab 
        #\Linefeed #\Page #\Return #\Rubout)
      (handler-case (with-output-to-string (s) (sb-ext:run-program "/bin/sh" (list "-c" "command -v git") :output s)) (simple-error () nil))))

